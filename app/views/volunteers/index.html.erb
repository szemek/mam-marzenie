<h1><%= :volunteers %></h1>
<div class="row">
  <div class="col-lg-6">
    <%= form_tag(search_volunteers_path, method: :get) do %>
      <div class="input-group">
        <span class="input-group-btn">
          <%= text_field_tag :fullname, nil, class: 'form-control'%>
          <%= submit_tag t('search'), class: 'btn btn-default' %>
        </span>
      </div>
    <% end %>
  </div>
</div>
<ul>
<% @users.each do |user| %>
  <li><%= user.fullname %></li>
<% end %>
</ul>
